Algorithm:
problem 1:
https://leetcode-cn.com/problems/palindrome-number/
solution 1:
class Solution {
public:
    bool isPalindrome(int x) {
        if(x < 0)
            return false;
        string str = to_string(x);
        for(int i = 0,j = str.size() - 1;i < j;i++,j--){
            if( str[i] != str[j] )
                return false;
        }
        return true;
    }
};
solution2 : 
class Solution {
public:
    bool isPalindrome(int x) {
        if(x < 0)
            return false;
        vector<int> ivec;
        while(x <= 0){
            int temp = x % 10;
            x /= 10;
            ivec.push_back(temp);
        }
        int i,j;
        for(i = 0,j = ivec.size() - 1;i < j;i++,j--){
            if(ivec[i] != ivec[j]){
                return false;
            }
        }
        return true; 
    }
};
problem 2:
https://leetcode-cn.com/problems/container-with-most-water/submissions/
solution 1:
class Solution {
public:
    int maxArea(vector<int>& height) {
        if(height.empty())
            return 0;
        int maxArea = 0;
        for(int i = 0;i < height.size();i++){
            for(int j = i + 1;j < height.size();j++){
                int width = j - i,length = 0,area = 0;
                if(height[i] > height[j]){
                    length = height[j];
                }else{
                    length = height[i];
                }
                area = width * length;
                if(area > maxArea){
                    maxArea = area;
                }
            }
        }
        return maxArea; 
    }
};
problem 3：
https://leetcode-cn.com/problems/integer-to-roman/
class Solution {
public:
    string intToRoman(int num) {
         int tenPow = 1;
         string resultString;
         string unitsDigit;
         string tensDigit;
         string hundredsDigit;
         string thousandsDigit;
         while(num > 0){
           int temporaryNumber = num % 10;
           temporaryNumber *= tenPow;
           num /= 10;
           if(0 < temporaryNumber && temporaryNumber < 10){
                      switch(temporaryNumber){
                          case 1 : unitsDigit = "I"; break;
                          case 2 : unitsDigit = "II"; break;
                          case 3 : unitsDigit = "III"; break;
                          case 4 : unitsDigit = "IV"; break;
                          case 5 : unitsDigit = "V"; break;
                          case 6 : unitsDigit = "VI"; break;
                          case 7 : unitsDigit = "VII"; break;
                          case 8 : unitsDigit = "VIII"; break;
                          case 9 : unitsDigit = "IX"; break;
                       }
            }//end of if
            else if( temporaryNumber >= 10 && temporaryNumber < 100){
                       switch(temporaryNumber){
                           case 10 : tensDigit = "X"; break;
                           case 20 : tensDigit = "XX"; break;
                           case 30 : tensDigit = "XXX"; break;
                           case 40 : tensDigit = "XL"; break;
                           case 50 : tensDigit = "L"; break;
                           case 60 : tensDigit = "LX"; break;
                           case 70 : tensDigit = "LXX"; break;
                           case 80 : tensDigit = "LXXX"; break;
                           case 90 : tensDigit = "XC"; break;
                       }
            }
            else if(temporaryNumber >= 100 &&temporaryNumber < 1000){
                       switch(temporaryNumber){
                           case 100 : hundredsDigit = "C"; break;
                           case 200 : hundredsDigit = "CC"; break;
                           case 300 : hundredsDigit = "CCC"; break;
                           case 400 : hundredsDigit = "CD"; break;
                           case 500 : hundredsDigit = "D"; break;
                           case 600 : hundredsDigit = "DC"; break;
                           case 700 : hundredsDigit = "DCC"; break;
                           case 800 : hundredsDigit = "DCCC"; break;
                           case 900 : hundredsDigit = "CM"; break;
                       }
            }
            else if(temporaryNumber >= 1000 && temporaryNumber < 4000){
                       switch(temporaryNumber){
                           case 1000 : thousandsDigit = "M"; break;
                           case 2000 : thousandsDigit = "MM"; break;
                           case 3000 : thousandsDigit = "MMM"; break;
                       }
            }
            tenPow *= 10;  
         }
        resultString = thousandsDigit + hundredsDigit + tensDigit + unitsDigit;
        return resultString;
    }
};

